{
  "manifest_version": 3,
  "name": "Tab Grouper Pro",
  "version": "1.31",
  "description": "Organize your tabs into groups automatically, with one click.",
  "permissions": [
    "tabs",
    "tabGroups"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/tab-icon.png",
      "48": "icons/tab-icon.png",
      "128": "icons/tab-icon.png"
    }
  },
  "icons": {
    "16": "icons/tab-icon.png",
    "48": "icons/tab-icon.png",
    "128": "icons/tab-icon.png"
  }
}
